{
    "name": "polymedia-chat",
    "author": "juzybits <https://twitter.com/juzybits>",
    "private": true,
    "dependencies": {
        "@emoji-mart/data": "1",
        "@emoji-mart/react": "1",
        "@mysten/sui.js": "latest",
        "@mysten/wallet-kit": "latest",
        "emoji-mart": "5",
        "react": "18",
        "react-dom": "18",
        "react-router-dom": "6"
    },
    "devDependencies": {
        "@parcel/transformer-less": "latest",
        "@types/react": "latest",
        "@types/react-dom": "latest",
        "buffer": "^5.5.0",
        "parcel": "latest",
        "process": "latest",
        "react-devtools": "latest"
    },
    "scripts": {
        "serve": "rm -rf .parcel-cache/ dist/ && parcel src/index.html",
        "build": "rm -rf .parcel-cache/ dist/ && parcel build --no-source-maps src/index.html && cp src/static/* dist/",
        "deploy-dev": "yarn build && wrangler pages publish dist/ --project-name polymedia-chat --branch dev",
        "deploy-prod": "yarn build && wrangler pages publish dist/ --project-name polymedia-chat --branch main"
    }
}
