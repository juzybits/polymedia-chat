{
    "name": "polymedia-web",
    "author": "juzybits <https://twitter.com/juzybits>",
    "private": true,
    "dependencies": {
        "@mysten/sui.js": "latest",
        "@mysten/wallet-adapter-all-wallets": "latest",
        "@mysten/wallet-adapter-react": "latest",
        "node-polyglot": "2",
        "react": "18",
        "react-dom": "18",
        "react-router-dom": "6"
    },
    "devDependencies": {
        "@parcel/transformer-less": "latest",
        "@types/node-polyglot": "2",
        "@types/react": "latest",
        "@types/react-dom": "latest",
        "parcel": "latest",
        "process": "latest",
        "react-devtools": "latest"
    },
    "scripts": {
        "serve": "rm -rf .parcel-cache/ && parcel src/index.html",
        "build": "rm -rf dist/ && parcel build --no-source-maps src/index.html && cp src/static/* dist/",
        "serve-teaser": "rm -rf .parcel-cache/ && parcel teaser/index.html",
        "build-teaser": "rm -rf dist/ && parcel build --no-source-maps teaser/index.html && cp src/static/* dist/"
    }
}
